  <select id="listProductsAndStocks" resultMap="productAndStocksResultMap">
    SELECT * FROM (
      SELECT BP.productId, BP.producingArea,
      BP.totalStorage, BP.productCode,
      BP.productName, BP.expiryDate,
      BP.unit, BP.standard,
      BP.tradePrice, BP.retailPrice,
