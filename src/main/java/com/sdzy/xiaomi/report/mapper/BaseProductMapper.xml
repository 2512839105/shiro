 <!--查看近期新品  -->
	<select id="selectBaseProductByMonth" resultType="java.util.Map">
	SELECT  productCode,productName,retailPrice 
         FROM baseProduct WHERE  storeId=#{storeId} and IFNULL(deleteFlag,0) != -1 
         AND TO_DAYS(NOW())-TO_DAYS(createDate) &lt; 30 
          GROUP BY productName ,storeId
	</select>
	<!-- 周 -->
	<select id="selectBaseProductByWeek" resultType="java.util.Map">
	SELECT  productCode,productName,retailPrice 
         FROM baseProduct WHERE  storeId=#{storeId} and IFNULL(deleteFlag,0) != -1 
         AND TO_DAYS(NOW())-TO_DAYS(createDate) &lt;14  
          GROUP BY productName storeId
	</select>
	<!-- 天 -->
	<select id="selectBaseProductByDay" resultType="java.util.Map">
	SELECT  productCode,productName,retailPrice 
         FROM baseProduct WHERE  storeId=#{storeId} and IFNULL(deleteFlag,0) != -1 
         AND TO_DAYS(NOW())-TO_DAYS(createDate) &lt; 7 
          GROUP BY productName storeId
	</select>
	<!--查询价格变动  -->
	<select id="baseProduct" resultType="java.util.Map">
	   select productId,productName,tradeOldPrice,tradePrice,productCode,totalStorage,localRetailPrice,memberPrice from baseProduct where  tradeOldPrice!=tradePrice and IFNULL(deleteFlag,0) != -1
	     
	   <if test="whereList != null"> 
 		 <foreach collection="whereList" item="item"> 
 			<choose> 
 				 <when test="item.opStr == 'in' ">and ${item.key} in   	
 				 	<foreach collection="item.value" item="val" open="(" separator="," close=")"> 
 				 	 	#{val}  	
 				 	 </foreach>   
 				 </when>   
 					<when test="item.opStr == 'like'">
 						 and ${item.key} ${item.opStr} CONCAT(#{item.value},'%')  
 					</when> 
 					<when test="item.key=='tradeOldPrice'">
 					  and ${item.key} ${item.opStr} ${item.value}  
 					</when>
 					  <otherwise>
 					    and ${item.key} ${item.opStr} #{item.value}  
 					  </otherwise> 
 			 </choose> 
 		</foreach>
 	 </if>
	</select>	
	<!--修改会员售价和 零售价 -->
	<update id="updateProduct">
	   update baseProduct set memberPrice=#{memberPrice},localRetailPrice=#{localRetailPrice}
	   where storeId=#{storeId} and productId=#{productId}
	</update>	
	<!--根据货号或商品名模糊查询  -->
	<select id="selectProductByCodeOrName" resultType="java.util.Map">
		select productId,productName,tradeOldPrice,tradePrice,productCode,totalStorage,localRetailPrice,memberPrice from baseProduct where 
		productName like CONCAT(CONCAT('%', #{keyword}), '%') or productCode like CONCAT(CONCAT('%', #{keyword}), '%') and storeId=#{storeId} and  IFNULL(deleteFlag,0) != -1
	    and status=1
	   		
	</select>
	
	<select id="findProductByBarCode" resultMap="BaseResultMap">
	    SELECT * FROM baseProduct
	    WHERE (retailPrice > 0 OR localRetailPrice > 0)
	    <if test="storeId != null">
	      AND  storeId=#{storeId}
	    </if>
	    <if test="isShowSpilt != null">
	      <![CDATA[ AND LEFT(productCode, 1) <> '9']]>
	    </if>
	    <if test="keyword != null">
	      AND (barCode = #{keyword} or localBarCode = #{keyword})
	    </if>
	    <if test="unit != null and unit != ''">
	      AND (unit = #{unit} OR LEFT(productCode, 1) = '5')
	    </if>
	    <if test="ignore == null">
	      AND IFNULL(isAudit,'') = 1
	    </if>
	    <!-- and isCancel = 0 -->
	    <!-- 禁用的商品一样可用 -->
	    <!-- and status = 1 -->
	    ORDER BY productCode ASC
  </select>
  <!--根据产品编码查询指导价大于0的产品 author:charlin-->
  <select id="findProductByProductCode" resultMap="BaseResultMap">
    SELECT * FROM baseProduct
    WHERE (retailPrice > 0 OR localRetailPrice > 0)
    <if test="storeId != null">
      AND  storeId=#{storeId}
    </if>
    <if test="isShowSpilt != null">
      <![CDATA[ AND LEFT(productCode, 1) <> '9']]>
    </if>
    <if test="keyword != null">
      AND (productCode = #{keyword})
    </if>
    <if test="unit != null and unit != ''">
      AND (unit = #{unit} OR LEFT(productCode, 1) = '5')
    </if>
    <if test="ignore == null">
      AND IFNULL(isAudit,'') = 1
    </if>
    ORDER BY productCode ASC
  </select>
