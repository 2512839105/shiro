<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdzy.xiaomi.serviceProduct.mapper.ServiceProductStatMapper">

	<select id="selectBybuyerId" resultType="java.util.Map">
	
